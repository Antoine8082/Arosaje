{% extends 'base.html.twig' %}
{% block body %}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Mouse page</title>
    <link rel="stylesheet" href="{{ asset('assets/css/profile.css') }}">
</head>
<body>

{% for userPost in posts %}
    <div id="card">
        <h3>{{ userPost.title}}</h3>
        <p>{{ userPost.description }}</p>
